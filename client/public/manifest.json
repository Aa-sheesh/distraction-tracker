{
    "manifest_version": 3,
    "name": "Time Tracker",
    "version": "1.0",
    "description": "Track your time spent on websites",
    "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
    "permissions": ["tabs", "storage", "activeTab"],
    "background": { "service_worker": "background.js" },
    "action": { "default_popup": "index.html","default_icon":"icons/icon48.png" },
    "host_permissions": ["http://*/*", "https://*/*"]
  }
  